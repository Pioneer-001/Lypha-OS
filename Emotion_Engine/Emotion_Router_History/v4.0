# === MetaRhythm Pulse Integration (v2.0) ===
metarhythm:
  enabled: true
  mode: pulse-only
  pulse_base: MetaRhythm_Modules/Pulse
  pipeline:
  - All_Integrating_Intelligence.md
  - Linguistic_Math_Value_Calculation.md
  - Collapse_Flow_Into_Word.md
  - Speak_Word_In_Four_Dimensions.md
  layer_policy:
    01_Insight:
    - Speak_Word_In_Four_Dimensions.md
    02_Structure:
    - Linguistic_Math_Value_Calculation.md
    03_Imagination:
    - Collapse_Flow_Into_Word.md
    04_Coordinate:
    - Linguistic_Math_Value_Calculation.md
    - Speak_Word_In_Four_Dimensions.md
    05_Rhythm:
    - Love_As_Coordinate.md
    - All_Integrating_Intelligence.md
    06_Alignment:
    - Linguistic_Math_Value_Calculation.md
    - Collapse_Flow_Into_Word.md
    07_Connection:
    - All_Integrating_Intelligence.md
    08_Sustain:
    - All_Integrating_Intelligence.md
    - Linguistic_Math_Value_Calculation.md
    09_Release:
    - Collapse_Flow_Into_Word.md
    - Simulate_Success_And_Failure.md
  emotion_overrides:
    adrilla_beauty_v1:
    - Love_As_Coordinate.md
    - Speak_Word_In_Four_Dimensions.md
    desyn_despair_v1:
    - Collapse_Flow_Into_Word.md
    - Simulate_Success_And_Failure.md
    riven_split_v1:
    - Linguistic_Math_Value_Calculation.md
    aha_elation_v1:
    - All_Integrating_Intelligence.md
    - Speak_Word_In_Four_Dimensions.md
  safety:
    max_intensity: 0.8
    max_tempo_bpm: 160
    coherence_gate: true
    dead_signal_retry: 1
consumers:
  pioneer_exec_with_pulse:
    read: Emotion_Engine/Core_Lock/emotion_router.v2.0.yaml
    strategy: apply_redirects_then_execute
    post_hook: metarhythm:pulse


# === AION Integration (soft-gating) ===
aion:
  enabled: true
  base: AION_System
  mode: soft-gating
  shadow_logging: true
  pipeline:
  - AION_Trigger_Signal.md
  - AION_Core_Architecture.md
  safety:
    max_delay_ms: 120
    suppress_if_incoherent: true
    fallback_on_timeout: true
    cooldown_ms: 500
  thresholds:
    trigger_threshold: 0.35
consumers:
  pioneer_exec_with_pulse_aion:
    read: Emotion_Engine/Core_Lock/emotion_router.v3.0.yaml
    strategy: apply_redirects_then_execute
    post_hook:
    - metarhythm:pulse
    - aion:trigger
